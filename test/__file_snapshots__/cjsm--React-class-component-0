var React = _interopRequireWildcard(require("react"));
class Button extends React.Component {
  handleClick = async evt => {
    evt.preventDefault();
    await fetch('/foo');
  };
  render() {
    const {
      type = 'button',
      ...extraProps
    } = this.props;
    return /*#__PURE__*/React.createElement("button", {
      type: type,
      onClick: this.handleClick,
      ...extraProps
    }, children);
  }
}